package lib.semantico;

public class Simbolo {
	
	// Representa el tipo de símbolo
	enum Tipo_simbolo { PROGRAMA, VARIABLE, ACCION, PARAMETRO };
	
	// Representa el tipo de variable
	enum Tipo_variable { DESCONOCIDO, ENTERO, BOOLEANO, CHAR, CADENA };
	
	// Representa la clase de los parámetros en las acciones
	enum Clase_parametro { VAL, REF };

}
